<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.austinhlee.spoileralert">

    <application
        android:allowBackup="true"
        android:icon="@drawable/spoiler_alert_logo_only"
        android:label="@string/app_name"
        android:roundIcon="@drawable/spoiler_alert_logo_only"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">SpoilerAlertLogoOnly.png
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ConfirmActivity"></activity>

    </application>

</manifest>